<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="fun.lianys.liuli.dao.UserDao">

    <select id="getUserById" parameterType="long" resultType="fun.lianys.liuli.pojo.User">
        select id, user_name
        from user
        where id=#{id}
    </select>

</mapper>